    "version": "0.2.0",
    "configurations": [

        {
            "name": "C++ Launch (Windows)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/main_launcher.exe",
            "symbolSearchPath": "C:\\Libraries\\cpp\\boost_1_68_0\\",
            "externalConsole": true,
            "logging": {
                "moduleLoad": false,
                "trace": true
             },
            "args": [],
            "cwd": "${workspaceFolder}",
            "environment": [],
            "linux": {
                "visualizerFile": "${workspaceRoot}/my.natvis",
                "showDisplayString": true,
                "MIMode": "gdb",
                "miDebuggerPath": "/usr/bin/gdb",
                "stopAtEntry": false,
            },
            "osx": {
                "visualizerFile": "${workspaceRoot}/my.natvis",
                "showDisplayString": true,
                "MIMode": "lldb",
                "stopAtEntry": false,
            },
            "windows": {
                "visualizerFile": "${workspaceRoot}/my.natvis",
                "showDisplayString": true,
                "MIMode": "gdb",
                "miDebuggerPath": "C:\\MinGw\\bin\\gdb.exe",
                "stopAtEntry": false,
            },
            "preLaunchTask": "build algorithm programme"

        }
    ]
}